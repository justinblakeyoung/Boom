

{
   "manifest_version": 2,
   "name": "Boom Feedback Scraper",
   "version": "0.1",
   "content_scripts": [
     {
       "matches": [
         "<all_urls>"
       ],
       "js": ["js/jquery-3.1.1.slim.min.js", "js/content.js"]
     }
   ],
   "browser_action": {
      "default_icon": "icon.png"
    },
    "background": {
      "scripts": ["js/background.js"]
    }
 }
 